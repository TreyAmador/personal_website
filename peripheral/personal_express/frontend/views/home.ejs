<!-- home page html -->

<!doctype html>
<html lang="en">
  <head>
    <% include ./partials/head.ejs %>
    <title>Trey Amador</title>
  </head>
  <body ng-app="personal">
    <header><% include ./partials/header.ejs %></header>
    <div class="home-page" ng-controller="homeCtrl">
      <div class="intro-banner">
        <div class="introduction">
          Hello, my name is <span>Trey</span>. 
          Welcome to my website. 
          Here, you can see a portfolio of my programming projects. 
        </div>
      </div>
      <div class="entries">


        <div class="entry entry-cengine">
          <div class="entry-image cengine-image"></div>
          <div class="entry-placard"></div>
          <div class="entry-headline">
            <div class="project">Game Engine</div>
            <div class="language">implemented in C++</div>
          </div>
        </div>

        <div class="entry entry-trader">
          <div class="entry-image trader-image"></div>
          <div class="entry-placard"></div>
          <div class="entry-headline">
            <div class="project">BroncoCorner.com</div>
            <div class="language">MEAN Stack website</div>
          </div>
        </div>

        <div class="entry entry-robot">
          <div class="entry-image robot-image"></div>
          <div class="entry-placard"></div>
          <div class="entry-headline">
            <div class="project">Robotics simulations</div>
            <div class="language">implemented in C++</div>
          </div>
        </div>

        <div class="entry entry-asteroids">
          <div class="entry-image asteroids-image"></div>
          <div class="entry-placard"></div>
          <div class="entry-headline">
            <div class="project">Asteroids clone</div>
            <div class="language">implemented in Java</div>
          </div>
        </div>




      </div>
    </div>
  </body>
</html>
